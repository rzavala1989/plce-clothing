{
  "name": "plce-clothing",
  "version": "1.0.0",
  "description": "serverside overview of plc app",
  "main": "server.js",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm server\" \"npm client\"",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rzavala1989/plce-clothing.git"
  },
  "author": "Ricardo Zavala",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rzavala1989/plce-clothing/issues"
  },
  "homepage": "https://github.com/rzavala1989/plce-clothing#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "stripe": "^8.167.0"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "nodemon": "^2.0.12"
  }
}
